{
  "name": "madebydaniz/location-info",
  "version": "1.0.1",
  "description": "This is a tool to search OSM data by name and address and to generate synthetic addresses of OSM points (reverse geocoding).",
  "license": "MIT",
  "type": "package",
  "keywords": [
    "wordpress",
    "composer",
    "location",
    "address",
    "OpenStreetMap"
  ],
  "require": {
    "php": ">=7.1",
    "curl/curl": "^2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*"
  },
  "authors": [
    {
      "name": "Daniel Niazmand",
      "email": "madebydaniz@gmail.com",
      "homepage": "https://madebydaniz.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "madebydaniz\\LocationInfo\\": "src/"
    }
  }
}
